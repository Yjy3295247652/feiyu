.swipers {
  width: 100%;
  height: 314rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.option_card {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.scroll {
  width: 750rpx;
  height: 100rpx;
  overflow: hidden;
  display: flex;
  flex-wrap: nowrap;
}

.item {
  height: 100rpx;
  padding: 29rpx 50rpx;
  position: relative;
  text-align: center;
  line-height: 100rpx;
  white-space: nowrap;
  font-size: 32rpx;
}

.item.active {
  height: 100rpx;
  color: #6eafdc;
}

.item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  height: 4rpx;
  width: 200rpx;
  background: #6eafdc;
}

.nohas {
  width: 100%;
  height: 200rpx;
  text-align: center;
  line-height: 200rpx;
  color: #6eafdc;
  font-size: 30rpx;
}

.bought {
  width: 180rpx;
  height: 222rpx;
  position: fixed;
  bottom: 60rpx;
  z-index: 2;
  background: url("data:image/png;base64,") no-repeat center/100%;
  transition: all 0.5s;
}

.yigou {
  color: #fff;
  font-size: 36rpx;
}

.yigou1 {
  margin-top: 36rpx;
}

.yigou2 {
  margin-top: 10rpx;
}

.zhezhao {
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

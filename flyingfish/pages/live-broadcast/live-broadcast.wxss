.header {
  width: 100%;
  height: 94rpx;
  margin-top: 38rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.header .icon {
  width: 22rpx;
  height: 36rpx;
  background: url(data:image/png;base64,)no-repeat center center/cover;
  margin-right: 20rpx;
  box-sizing: border-box;
}

.video {
  width: 100%;
  height: 380rpx;
}

.video image {
  width: 100%;
  height: 100%;
}

.zhibo-name {
  width: 100%;
  height: 140rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 28rpx;
  color: #c7c5cd;
  padding: 0 24rpx;
  box-sizing: border-box;
  border-bottom: 20rpx solid #f1f5f8;
}

.zhibo-top {
  font-size: 30rpx;
  color: #000;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.zhibo-bottom {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.zhibo-bottom text {
  margin-right: 10rpx;
  box-sizing: border-box;
}

.zhibo-bottom .zhibo-icon {
  width: 38rpx;
  height: 34rpx;
  background: url(data:image/png;base64,)no-repeat center center/cover;
}

.enter {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #428df3;
  text-align: center;
  line-height: 100rpx;
  font-size: 38rpx;
  color: #fff;
}

.option_card {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.navbar {
  flex: none;
  display: flex;
  background: #fff;
}

.navbar .item {
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 100rpx;
}

.navbar .item.active {
  color: #6eafdc;
}

.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  height: 4rpx;
  width: 200rpx;
  background: #6eafdc;
}

.list {
  width: 100%;
  height: auto;
}

.list-lis {
  width: 100%;
  height: auto;
  padding-left: 24rpx;
  box-sizing: border-box;
}

.list_lis_li {
  width: 100%;
  /* height: 150rpx; */
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-bottom: 2rpx solid #ececec;
  box-sizing: border-box;
}

.li_content {
  /* height: 144rpx; */
  padding-right: 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.li_content_top {
  width: 100%;
  height: 128rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.h1 {
  width: 100%;
  height: auto;
  font-size: 30rpx;
  color: #333;
  overflow: hidden;
  font-weight: bold;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.time {
  width: 100%;
  line-height: 26rpx;
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #89d0e0;
  display: flex;
}

.li_content_bottom {
  color: #ccc;
  font-size: 28rpx;
  line-height: 30rpx;
  display: flex;
  justify-content: flex-start;
  position: relative;
  margin-left: 20rpx;
  box-sizing: border-box;
}

.cost {
  font-size: 28rpx;
  color: #89d0e0;
  position: absolute;
  bottom: 0;
  right: 24rpx;
}

.price {
  position: absolute;
  bottom: 0;
  right: 24rpx;
  font-size: 20rpx;
  color: #e5636c;
  display: flex;
  justify-content: flex-end;
  align-items: flex-end;
  line-height: 30rpx;
}

.price_text {
  line-height: 26rpx;
}

.price_number {
  font-size: 30rpx;
}

.baseline {
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 24rpx;
  color: #ababab;
}

.jie {
  width: 100%;
  height: auto;
  padding: 10rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
}
.icon-jiao{
  width: 30rpx;
  height: 30rpx;
  margin: 6rpx 10rpx 14rpx 50rpx;
  background: url(data:image/png;base64,) no-repeat center / 30rpx auto;
}
.jie-dian {
  font-size: 30rpx;
  color: #333;
  
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

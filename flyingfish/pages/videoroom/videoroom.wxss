#myVideo {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: #000;
}
.myVideo{
  width: 100%;
  height: 225px;
  position: fixed;
  left: 0;
  top: 0;
  background: #000;
}
.liuyan {
  width: 100%;
  height: auto;
  padding:225px 0 100rpx 0; 
}
.btn-area {
  width: 100%;
  height: 100rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  background: #1296db;
}

.btn-area .bottom {
  width: 100%;
  height: 100%;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}

.btn-area .bottom input {
  width: 100%;
  height: 80%;
  display: flex;
  align-items: center;
}

.btn-area .bottom input {
  height: 80rpx;
  width: 702rpx;
  border-radius: 50rpx;
  padding: 0 20rpx 0 40rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  outline: none;
  border: none;
  background: #fff;
  z-index: 200;
}

button::after {
  border: none;
  padding: 0;
}

.btn-area .bottom .danmu {
  width: 78rpx;
  height: 78rpx;
  position: absolute;
  right: 26rpx;
  top: 13rpx;
  z-index: 999;
  background: #fff url(data:image/png;base64,)no-repeat center center /50rpx auto;
}

.list {
  width: 100%;
  height: auto;
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  margin: 15rpx 0;
  border-bottom: 1rpx solid #f0f0f0;
}

.list:last-child {
  border-bottom: 0;
}

.list-img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}

.list-img image {
  width: 100%;
  height: 100%;
}

.list-content {
  width: 570rpx;
  min-height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 30rpx;
  color: #333;
  margin-left: 30rpx;
}

.list-name {
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.list-name .name {
  font-size: 40rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.list-name .time {
  font-size: 30rpx;
}

.list-text {
  width: 100%;
  min-height: 60rpx;
  display: flex;
  justify-content: space-between;
  margin: 20rpx 0;
  font-size: 34rpx;
}

.list-text .text {
  display: flex;
  align-items: center;
  overflow: hidden;
  white-space: wrap;
}

.icon {
  display: flex;
  justify-content: flex-end;
}

.icon .huifu {
  width: 80rpx;
  height: 80rpx;
  background: url(data:image/png;base64,)no-repeat center center / 40% auto;
}

.zan {
  display: flex;
  align-items: center;
}

.zan-icon {
  width: 60rpx;
  height: 60rpx;
  background: no-repeat center center / 60% auto;
}
.zan-icon1{
  background-image: url(data:image/png;base64,);
}
.zan-icon2{
  background-image: url(data:image/png;base64,);
}
.all-reply{
  background: #fff;
  width: 100%;
  height: calc(100vh - 225px);
  position: fixed;
  left: 0;
  bottom: 0;
  padding:24rpx 24rpx 0rpx;
  box-sizing: border-box;
}
.all-reply .main{
  width: 100%;
  display: flex;
}
.all-reply .main image{
  width: 100rpx;
  height: 100rpx;
  background: yellow;
  border-radius: 50%;
}
.all-reply .des{
  margin-left: 24rpx;
  width: calc(100% - 148rpx);
}
.all-reply .des-top{
  display: flex;
  justify-content: space-between;
}
.all-reply .des-top .user-name{
  font-size: 40rpx;
  width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.all-reply .reply-btn{
  background: url(data:image/png;base64,)no-repeat center center / 40% auto;
  width: 80rpx;
  height: 80rpx;
}
.all-reply .des-bottom{
  padding: 10rpx 0 ;
  line-height: 42rpx;
  font-size: 32rpx;
  color: #ccc;
}
.reply-list{
  padding: 24rpx 5rpx;
  background: #f3f3f3;
  font-size: 32rpx;
}
.reply-list .list-item{
  margin-bottom:10rpx;
}
.reply-list .list-item:last-child{
  margin-bottom: 0;
}
.reply-list .user-name{
  color: #1296db;
  letter-spacing: 2px;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.reply-all-icon{
  display: flex;
  align-items: center;
}
.no-reply{
  font-size: 32rpx;
  color: #ccc;
  text-align: center;
  margin-top: 20px;
}

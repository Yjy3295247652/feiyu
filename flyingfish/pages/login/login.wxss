.di {
  width: 100%;
  height: 100%;
  position: fixed;
  bottom: 0;
  top: 0;
  left: 0;
  z-index: -1;
}

.di image {
  display: block;
  width: 100%;
  height: 100%;
}

.head {
  width: 200rpx;
  height: 300rpx;
  position: absolute;
  top: 160rpx;
  margin-left: 50%;
  left: -100rpx;
  font-size: 40rpx;
  text-align: center;
  color: #7bc2d3;
  z-index: 200;
}

.head-img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

.head-img image {
  display: block;
  width: 100%;
  height: 100%;
}

.login {
  width: 100%;
  height: 100%;
  z-index: 100;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
}

.login .kuang {
  width: 80%;
  height: 450rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.user {
  display: flex;
  margin-bottom: 50rpx;
}

.user .user-icon {
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,)no-repeat center center/cover;
}

.code {
  display: flex;
  margin-bottom: 80rpx;
}

.code .code-icon {
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,)no-repeat center center /cover;
}

.code-input input {
  width: 280rpx;
}

.code-input .code_button {
  width: 200rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #5778f1;
  border-radius: 25rpx;
  color: #fff;
}

.user-input, .code-input {
  width: 450rpx;
  border-bottom: 2rpx solid #fff;
  margin-left: 20rpx;
  box-sizing: border-box;
  display: flex;
}

.btns {
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.button {
  width: 220rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 30rpx;
  background: #7dd4e8;
  opacity: 0.8;
  color: #fff;
}

.button1 , .button2 {
  width: 300rpx;
  height: 60rpx;
  border-radius: 30rpx;
  background: #7dd4e8;
  opacity: 0.8;
  color: #fff;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-left: -150rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 32rpx;
}
.button2{
  top: 60%;
}



